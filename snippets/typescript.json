{
    "WeApp app": {
        "prefix": "wa-app",
        "body": [
            "let param: AppParam = {",
            "\tonLaunch: function () {",
            "\t\t$1",
            "\t},",
            "\tonShow: function () {",
            "\t\t$2",
            "\t},",
            "\tonHide: function () {",
            "\t\t$3",
            "\t}",
            "};",
            "",
            "App(param);"
        ]
    },
    "WeApp page": {
        "prefix": "wa-page",
        "body": [
            "let self;",
            "",
            "let param: PageParam = {",
            "\tdata: {$0},",
            "\tonLoad: function () {",
            "\t\tself = this;",
            "\t}",
            "};",
            "",
            "Page(param);"
        ]
    }
}